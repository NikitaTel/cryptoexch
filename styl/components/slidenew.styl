input#banner-input2:checked ~ .banner_dots #banner-label2,
input#banner-input3:checked ~ .banner_dots #banner-label3,
input#banner-input4:checked ~ .banner_dots #banner-label4,
input#banner-input5:checked ~ .banner_dots #banner-label5,
input#banner-input6:checked ~ .banner_dots #banner-label6
    color $blue

.swiper-pagination
    display flex
    position static
    &-bullet
        width 16px
        height 16px
        background #fff
        border 3px solid #000
        &-active
            background #fff
            border 4px solid #676362
            opacity 1

.cert-mc
    .swiper-button-next:after, .swiper-button-prev:after
        font-size 19px
        color: rgba(0,0,0,.7)


.exchange
    .swiper-button-next, .swiper-button-prev
        background rgba(0,0,0,.35)
        border-radius 100%
        padding 30px
        color #fff
        opacity 0.7
        &:after
            font-size 20px
    .swiper-button-next
            right -30px
            &:after
                padding-right 18px
    .swiper-button-prev
            left -33px
            &:after
                padding-left 26px

.certificates-slider__item img
    max-width 76%

@media screen and (max-width: 1200px)
    .slides6
        width 100%
        padding-bottom 120px
        .swiper-pagination
            position absolute
            &-bullet
                background #333
                border 4px solid #333
                width 14px
                height 14px
                &-active
                    background #314363
                    border 4px solid #c1c6d0
                    width 17px
                    height 17px
        .swiper-button-next, .swiper-button-prev
            top 27%
    .slides6_wrapper
        width 100%
        display flex
        flex-direction row !important
    .certificates-slider__item
        width 100%
    .main-content > div > .slides2
        width 90%
    .slides2
        max-width 580px
        width 100%
        height 100%
        &_wrapper
            .slide2
                &__close-quotes
                    bottom 20px
                    right 30px
                &__open-quotes
                    left 30px
    .news-more.mobile
        display flex
        justify-content center
        width 180px
        margin 30px auto 50px
        position static
        transform none
    .swiper-pagination
        justify-content center
    .messege
        height auto
        overflow auto
    .slide2
        min-height 290px
        height 100%
    .slides3
        max-width 400px
        width 100%
        &:nth-child(n+3)
            display none
    .slide4:nth-child(1)
        .money
            justify-content normal
            &_name
                align-items center
                padding-top 0
                > span
                    margin-right 10px
    .slides5
        max-width 400px
        width 100%
    .slide5
        .money_name_item.changeBtn, .money_change
            display none
        .slides5:nth-child(3)
            display none
    .change_get
        margin-top 40px
    .money
        &_name
            flex-wrap wrap
            flex-direction row
            flex-shrink unset
            flex-grow unset
            padding-top 20px
        & .price
            justify-content flex-end
            padding 5px 36px 10px 0
        & .money_name_item
            color #aaaaaa
            font-size 12px
            border 1px solid #aaaaaa