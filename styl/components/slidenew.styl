.slides ,.slides2 ,.slides3 ,.slides5 ,.slides6,.slides7
    display: block;
    margin: auto;
    position: relative;

.slides
    width: 580px;
    height: 224px;
    overflow hidden
    position relative
    &_wrapper
        width 3480px
        position relative
        transition transform .5s
        .slide-container
            width 580px
    .i-next, .i-back
        display none
        position absolute
        color #000
        opacity .7
        z-index 15
        top 50%
        transform translateY(-50%)
        font-size 30px
        &:hover
            opacity 1
    .i-next
        right 15px
    .i-back
        left 15px

input
    &#img-1
        &:checked
            ~.slides_wrapper
                transform translateX(0px)
            ~.nav-dots #img-dot-1
                background $blue
                opacity 1
            ~#nav-next-2, ~#nav-prev-6
                display block
    &#img-2
        &:checked
            ~.slides_wrapper
                transform translateX(-580px)
            ~.nav-dots #img-dot-2
                background $blue
                opacity 1
            ~#nav-next-3, ~#nav-prev-1
                display block
    &#img-3
        &:checked
            ~.slides_wrapper
                transform translateX(-1160px)
            ~.nav-dots #img-dot-3
                background $blue
                opacity 1
            ~#nav-next-4, ~#nav-prev-2
                display block
    &#img-4
        &:checked
            ~.slides_wrapper
                transform translateX(-1740px)
            ~.nav-dots #img-dot-4
                background $blue
                opacity 1
            ~#nav-next-5, ~#nav-prev-3
                display block
    &#img-5
        &:checked
            ~.slides_wrapper
                transform translateX(-2320px)
            ~.nav-dots #img-dot-5
                background $blue
                opacity 1
            ~#nav-next-6, ~#nav-prev-4
                display block
    &#img-6
        &:checked
            ~.slides_wrapper
                transform translateX(-2900px)
            ~.nav-dots #img-dot-6
                background $blue
                opacity 1
            ~#nav-next-1, ~#nav-prev-5
                display block

/*SLIDER 2*/

.slides2
